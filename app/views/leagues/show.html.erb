<section id="main-content">
  <section class="wrapper">
    <h3><i class="fa fa-angle-right"></i> League Results for <%= @league.name %></h3>
    <div class="row">

      <div class="col-md-12 mt">
        <div class="content-panel">
          <table class="table table-hover">
            <h4><i class="fa fa-angle-right"></i> Hover Table</h4>
            <hr>
            <thead>
              <tr>
                <th><i class="fa fa-bullhorn"></i> Category</th>
                <th><i class="fa fa-question-circle"></i> Data</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>Best Player by Points</td>
                <td>Mark</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </section>
</section>

